    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "[XHProf-Analyzer.js] Example 01",
            "program": "${workspaceFolder}/src/CLI.ts",
            "sourceMaps": true,
